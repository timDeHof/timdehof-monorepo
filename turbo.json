{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*", "src/**"],
			"outputs": ["dist/**", ".next/**", "!.next/cache/**"]
		},
		"@repo/ui#build": {
			"outputLogs": "hash-only",
			"inputs": ["src/**"],
			"outputs": ["dist/**"]
		},
		"lint": {},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
